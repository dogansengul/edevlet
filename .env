# Proxy ayarları - Devre dışı bırakıldı
# PROXY_LIST=http://proxy1.example.com:8080,http://proxy2.example.com:8080

# Tarayıcı ayarları
HEADLESS=false
WINDOW_SIZE=1920,1080

# Bekleme süreleri (saniye)
MIN_WAIT=1
MAX_WAIT=3
PAGE_LOAD_WAIT=5
ELEMENT_LOAD_WAIT=2

# İnsan benzeri davranış ayarları
MOUSE_MOVEMENT_SPEED=0.8
SCROLL_SPEED=0.5
TYPING_SPEED=0.2
PAUSE_BETWEEN_ACTIONS=1.0 